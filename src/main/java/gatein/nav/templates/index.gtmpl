<div class="gtn" style="margin-left: 8px;margin-right: 8px; padding: 4px">
  <ul class="nav nav-pills" style="margin-bottom: 0px">
  <% nodes.each { name, url ->
     def active = url == "#" ? "active" : "" %>
      <li class="${active}">
        <a href="${url}">${name}</a>
      </li>
  <% } %>
  </ul>
</div>